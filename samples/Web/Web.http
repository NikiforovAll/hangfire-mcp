        "userName": "John Doe"
    }
}

###
POST {{host}}/jobs/
Content-Type: application/json

{
    "typeName": "HangfireJobs.ITimeJobWithParameters",
    "methodName": "ExecuteAsync",
    "parameters": {
        "payload": {
            "userName": "John Doe",
            "age": 30
        }
    }
}
